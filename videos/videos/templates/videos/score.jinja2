{% include 'templates/layout.jinja2'%}
{% block content %}
<div class="row">
        <div class="col text-center">           
        </div>
        <div class="col-3">
        
            <ul class="list-group">
                {% for score in scores %}
                    <li class="list-group-item d-flex justify-content-between align-items-center">
                        {{score['_id']}} <span class="badge badge-success">{{score['total_score']}}</span>
                    </li>
                {% endfor %}
                
            </ul>
        </div>
        <div class="col">
        </div>
    </div>
{% endblock %}